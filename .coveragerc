[run]
branch = True
source = locking/
omit =
    # Ignore migrations
    */migrations/*

    # Ignore requirement files
    requirements/*

    # Ignore manage files
    manage*

    # Ignore admin files
    django-triggers/*/admin.py
[report]
exclude_lines=
    pragma: no cover
    def __repr__
    def __iter__
    def __str__
    def __unicode__
#fail_under = 99
